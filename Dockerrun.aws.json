{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "xbumasa/fibonacci-ui",
      "hostname": "ui",
      "essential": false
    },
    {
      "name": "api",
      "image": "xbumasa/fibonacci-api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "xbumasa/fibonacci-worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "xbumasa/fibonacci-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["ui", "api"]
    }
  ]
}
